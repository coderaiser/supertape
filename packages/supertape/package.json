{
  "name": "supertape",
  "version": "6.3.1",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "tape compatible test runner with superpowers",
  "homepage": "http://github.com/coderaiser/supertape",
  "main": "./lib/supertape.js",
  "exports": {
    ".": {
      "node": {
        "require": "./lib/supertape.js",
        "import": "./lib/supertape.mjs"
      },
      "default": "./lib/supertape.js"
    },
    "./bin/supertape": "./bin/supertape.mjs",
    "./cli": "./lib/cli.js"
  },
  "type": "commonjs",
  "bin": {
    "tape": "bin/supertape.mjs",
    "supertape": "bin/supertape.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/supertape.git"
  },
  "scripts": {
    "test": "madrun test",
    "test:dts": "madrun test:dts",
    "watch:test": "madrun watch:test",
    "lint": "madrun lint",
    "fresh:lint": "madrun fresh:lint",
    "lint:fresh": "madrun lint:fresh",
    "fix:lint": "madrun fix:lint",
    "coverage": "madrun coverage",
    "report": "madrun report",
    "wisdom": "madrun wisdom"
  },
  "dependencies": {
    "@cloudcmd/stub": "^3.0.0",
    "@putout/cli-keypress": "^1.0.0",
    "@putout/cli-validate-args": "^1.0.1",
    "@supertape/engine-loader": "^1.0.0",
    "@supertape/formatter-fail": "^2.0.0",
    "@supertape/formatter-json-lines": "^1.0.0",
    "@supertape/formatter-progress-bar": "^2.0.0",
    "@supertape/formatter-short": "^1.0.0",
    "@supertape/formatter-tap": "^2.0.0",
    "@supertape/operator-stub": "^1.0.0",
    "chalk": "^4.1.0",
    "cli-progress": "^3.8.2",
    "deep-equal": "^2.0.3",
    "fullstore": "^3.0.0",
    "glob": "^7.1.6",
    "jest-diff": "^27.0.1",
    "once": "^1.4.0",
    "resolve": "^1.17.0",
    "simport": "^1.1.1",
    "stacktracey": "^2.1.7",
    "strip-ansi": "^7.0.0",
    "try-to-catch": "^3.0.0",
    "wraptile": "^3.0.0",
    "yargs-parser": "^20.2.4"
  },
  "keywords": [
    "function",
    "promise",
    "async",
    "await",
    "then",
    "tap",
    "tape",
    "testing"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@iocmd/wait": "^1.0.0",
    "c8": "^7.3.5",
    "check-dts": "^0.5.5",
    "eslint": "^8.0.0-beta.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-putout": "^10.0.1",
    "madrun": "^8.0.0",
    "mock-require": "^3.0.2",
    "montag": "^1.0.0",
    "nodemon": "^2.0.2",
    "pullout": "^4.0.0",
    "putout": "^20.0.1",
    "runsome": "^1.0.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
